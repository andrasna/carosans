{
  "extends": "airbnb-base",
  "rules": {
    "semi": [2, "never"],
    "import/prefer-default-export": "off",
    "import/extensions": ["error", {"js": "always"}],
    "yoda": ["error", "never", { "onlyEquality": true }]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}